{
    "name": "click/elemental",
    "description": ":description",
    "license": "proprietary",
    "authors": [
        {
            "name": "Click",
            "email": "ops@clickdigitalsolutions.co.uk",
            "homepage": "https://github.com/clickops"
        }
    ],
    "homepage": "https://github.com/clickops/elemental",
    "keywords": ["Laravel", "Elemental"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Click\\Elemental\\": "src/",
            "Click\\Elemental\\Database\\Seeds\\": "database/seeds/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Click\\Elemental\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Click\\Elemental\\ElementalServiceProvider"
            ],
            "aliases": {
                "Elemental": "Click\\Elemental\\Facades\\Elemental"
            }
        }
    }
}
